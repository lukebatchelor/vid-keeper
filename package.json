{
  "name": "keep-vid",
  "version": "0.1.2",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "release:cut": "yarn version --no-git-tag-version --patch",
    "release:pull": "curl \"https://github.com/lukebatchelor/vid-keeper/releases/download/v$npm_package_version/release.tgz\" -sL -o release.tgz; tar -zxvf release.tgz --strip-components=1 package",
    "postrelease:cut": "git commit -am \"New release: $npm_package_version\""
  },
  "dependencies": {
    "@material-ui/core": "^4.11.4",
    "@material-ui/icons": "^4.11.2",
    "next": "11.0.1",
    "pretty-bytes": "^5.6.0",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "youtube-dl-exec": "^1.2.4"
  },
  "files": [
    ".next"
  ],
  "devDependencies": {
    "@types/react": "^17.0.13",
    "eslint": "7.30.0",
    "eslint-config-next": "11.0.1",
    "husky": "^7.0.0",
    "lint-staged": "^11.0.0",
    "prettier": "^2.3.2",
    "typescript": "^4.3.5"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,css,md,ts,tsx,json}": "prettier --write"
  },
  "prettier": {
    "singleQuote": true,
    "printWidth": 120
  }
}
